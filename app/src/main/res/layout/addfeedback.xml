<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="93dp"
            android:layout_height="57dp"
            android:layout_marginLeft="10dp"
            android:adjustViewBounds="false"
            android:cropToPadding="false"
            android:src="@drawable/finallogo" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="APTRI Labs"
            android:textSize="20dp"
            android:gravity="center"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp"
            android:textSize="25dp"

            android:background="@drawable/gradient"
            android:textColor="#fff"
            android:text="Participant's Feedback Form"
            android:gravity="center"/>

        <TextView
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textStyle="bold"
            android:text="HR/LD/T-FEED/05"/>


        <EditText
            android:layout_margin="25dp"
            android:id="@+id/traid"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Enter ID"/>

        <EditText
            android:layout_margin="25dp"
            android:id="@+id/traname"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Enter Name"/>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Select Training : "/>
        <Spinner
            android:layout_marginLeft="5dp"
            android:id="@+id/title"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
</LinearLayout>

<Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="GET DATA"
    android:onClick="getdata"
    android:layout_marginTop="20dp"
    android:layout_gravity="center"/>

        <LinearLayout
            android:layout_marginTop="20dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Start Date: "/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/startdated"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/startdatem"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/startdatey"/>

        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="20dp"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="End Date: "/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/enddated"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/enddateem"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/enddatey"/>

        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="40dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Start Time: "/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/starttimeh"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/starttimem"/>

        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="20dp"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="End Time: "/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/endtimeh"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/endtimem"/>


        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="20dp"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
            <TextView
                android:layout_marginTop="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Location: "/>

            <TextView
                android:layout_marginTop="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/loc"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_marginTop="25dp"
            android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Select Faculty: "/>

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/faculties"
            android:layout_marginLeft="5dp"
            android:layout_gravity="center"
           />

        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
           android:gravity="center"
            android:text="Please mention your feedback on 1 to 5 scale"/>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:textStyle="bold"
            android:text="(1 = Satisfactory, 2 = Good , 3 = Very Good, 4 = V. Very Good, 5 = Excellent)"/>
        </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:textStyle="bold"
            android:text="1. Program Content"/>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_height="wrap_content"
            android:text="Program content met my learning objective "/>

        <Spinner

            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:entries="@array/ratings"
            android:id="@+id/a1"
            android:scrollbarSize="10dp"
            android:layout_height="wrap_content">

        </Spinner>
        </RelativeLayout>



            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            <TextView
                android:layout_alignParentLeft="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Content was simple and easy to understand"/>

            <Spinner
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:entries="@array/ratings"
                android:scrollbarSize="10dp"
                android:id="@+id/a2"
                android:layout_height="wrap_content">

            </Spinner>
            </RelativeLayout>




            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            <TextView
                android:layout_alignParentLeft="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Program content met my learning objective " />

            <Spinner
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:entries="@array/ratings"
                android:id="@+id/a3"
                android:scrollbarSize="10dp"
                android:layout_height="wrap_content">

            </Spinner>
            </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:textStyle="bold"
            android:text="2. Program Faculty"/>

            <RelativeLayout
                android:layout_width="wrap_content"

                android:layout_height="wrap_content">
            <TextView
                android:layout_alignParentLeft="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Training aids(Graphs, Role Play, Slides) used :"/>

            <Spinner
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:entries="@array/ratings"
                android:id="@+id/b1"
                android:scrollbarSize="20dp"
                android:layout_height="wrap_content">

            </Spinner>
            </RelativeLayout>



            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            <TextView
                android:layout_alignParentLeft="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Faculty's Knowledge and Query Solving Ability"/>

            <Spinner
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:entries="@array/ratings"
                android:id="@+id/b2"
                android:scrollbarSize="20dp"
                android:layout_height="wrap_content">

            </Spinner>
            </RelativeLayout>



            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            <TextView
                android:layout_alignParentLeft="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Ability to facilitate and encourage interaction  "/>

            <Spinner
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:entries="@array/ratings"
                android:id="@+id/b3"
                android:scrollbarSize="20dp"
                android:layout_height="wrap_content">

            </Spinner>
            </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:textStyle="bold"
            android:text="3. Program Coordination"/>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">
            <TextView
                android:layout_alignParentLeft="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Pre program Communication                "/>

            <Spinner
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:entries="@array/ratings"
                android:scrollbarSize="20dp"
                android:id="@+id/c1"
                android:layout_height="wrap_content">

            </Spinner>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_alignParentLeft="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Program facilitation and Coordination"/>

            <Spinner
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:entries="@array/ratings"
                android:id="@+id/c2"
                android:scrollbarSize="20dp"
                android:layout_height="wrap_content">

            </Spinner>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_alignParentLeft="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Program venue and other facilities      "/>

            <Spinner
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:entries="@array/ratings"
                android:id="@+id/c3"
                android:scrollbarSize="20dp"
                android:layout_height="wrap_content">

            </Spinner>
        </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:textStyle="bold"
            android:text="4. Program Outcome"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_height="wrap_content"
            android:text="Mention one area of practical Implementation of knowledge/skill learnt and specify time limit"/>



            <EditText
                android:layout_marginTop="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/d1"
                android:background="@drawable/rounded"
                android:padding="4dp"
                android:layout_gravity="center"
                android:hint="          Write area            "
                />

            <EditText
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:layout_width="wrap_content"
                android:padding="4dp"
                android:layout_gravity="center"
                android:inputType="number"
                android:id="@+id/d2"
                android:background="@drawable/rounded"
                android:layout_height="wrap_content"
                android:hint="Write time(in days)"
                />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:textStyle="bold"
            android:text="5. Self Learning Outcome"/>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">
            <TextView
                android:layout_alignParentLeft="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Rate your skill before program "/>

            <Spinner
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"

                android:entries="@array/ratings"

                android:scrollbarSize="20dp"
                android:id="@+id/e1"
                android:layout_height="wrap_content">

            </Spinner>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:orientation="horizontal">
            <TextView
                android:layout_alignParentLeft="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Rate your skill after program    "/>

            <Spinner
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:entries="@array/ratings"
                android:scrollbarSize="20dp"
                android:id="@+id/e2"
                android:layout_height="wrap_content">

            </Spinner>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:orientation="horizontal">
            <TextView
                android:layout_alignParentLeft="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Overall Rating for program        "/>

            <Spinner
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:entries="@array/ratings"
                android:scrollbarSize="20dp"
                android:id="@+id/e3"
                android:layout_height="wrap_content">

            </Spinner>
        </RelativeLayout>



        <Button
            android:layout_marginTop="25dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Submit"
            android:onClick="submit"
            android:layout_gravity="center"/>


    </LinearLayout>
</ScrollView>