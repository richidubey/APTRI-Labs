<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aptrifeedback">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".result"></activity>
        <activity android:name=".addtraining"></activity>
        <activity android:name=".addfeedback"></activity>
        <activity android:name=".addnewadmin"></activity>
        <activity android:name=".admincheck"></activity>
        <activity android:name=".inadmin"></activity>
        <activity android:name=".addfaculty"></activity>
        <activity android:name=".facultycheck"></activity>
        <activity android:name=".checkin"></activity>
        <activity android:name=".checkout"></activity>
        <activity android:name=".infaculty"></activity>
        <activity android:name=".seeattendance"></activity>
        <activity android:name=".deleteatt"></activity>
        <activity android:name=".deletefeedback"></activity>
        <activity android:name=".deletetraining"></activity>
    </application>

</manifest>